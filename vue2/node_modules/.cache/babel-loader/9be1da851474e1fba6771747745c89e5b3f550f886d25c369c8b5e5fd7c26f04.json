{"ast":null,"code":"import { createApp } from 'vue';\nimport App from './App.vue';\nimport { createRouter, createWebHistory } from 'vue-router';\nimport routes from './routes';\ncreateApp(App).use(createRouter({\n  history: createWebHistory(),\n  routes\n})).mount('#app');\n\n// Register the remote module\nif (window.__MICRO_APP__) {\n  window.__MICRO_APP__.register({\n    name: 'remoteApp',\n    module: () => import('remoteApp/App.vue'),\n    active: true,\n    shared: ['vue', 'vue-router']\n  });\n}","map":{"version":3,"names":["createApp","App","createRouter","createWebHistory","routes","use","history","mount","window","__MICRO_APP__","register","name","module","active","shared"],"sources":["C:\\xampp\\htdocs\\project-soa\\vue2\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <h1>BAG PULA</h1>\n    <p>Message from remote app: APROB</p>\n  </div>\n</template>\n\n<script>\nimport { createApp } from 'vue';\nimport App from './App.vue';\nimport { createRouter, createWebHistory } from 'vue-router';\nimport routes from './routes';\n\ncreateApp(App)\n  .use(createRouter({\n    history: createWebHistory(),\n    routes,\n  }))\n  .mount('#app');\n\n// Register the remote module\nif (window.__MICRO_APP__) {\n  window.__MICRO_APP__.register({\n    name: 'remoteApp',\n    module: () => import('remoteApp/App.vue'),\n    active: true,\n    shared: ['vue', 'vue-router'],\n  });\n}\n</script>\n"],"mappings":"AAQA,SAASA,SAAQ,QAAS,KAAK;AAC/B,OAAOC,GAAE,MAAO,WAAW;AAC3B,SAASC,YAAY,EAAEC,gBAAe,QAAS,YAAY;AAC3D,OAAOC,MAAK,MAAO,UAAU;AAE7BJ,SAAS,CAACC,GAAG,EACVI,GAAG,CAACH,YAAY,CAAC;EAChBI,OAAO,EAAEH,gBAAgB,EAAE;EAC3BC;AACF,CAAC,CAAC,EACDG,KAAK,CAAC,MAAM,CAAC;;AAEhB;AACA,IAAIC,MAAM,CAACC,aAAa,EAAE;EACxBD,MAAM,CAACC,aAAa,CAACC,QAAQ,CAAC;IAC5BC,IAAI,EAAE,WAAW;IACjBC,MAAM,EAAEA,CAAA,KAAM,MAAM,CAAC,mBAAmB,CAAC;IACzCC,MAAM,EAAE,IAAI;IACZC,MAAM,EAAE,CAAC,KAAK,EAAE,YAAY;EAC9B,CAAC,CAAC;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}